{
    "compilerOptions": {
      "target": "ES2020",
      "outDir": "lib",
      "lib": ["ES2020", "DOM", "DOM.Iterable"],
			"module": "ESNext",
			"skipLibCheck": true,

			"moduleResolution": "bundler",
			"resolveJsonModule": true,
      "isolatedModules": true,
			"noEmit": true,
      "declaration": true,
      "declarationDir": "lib",
      "allowJs": true,
			"jsx": "react-jsx",
    },
    "include": ["src"],
    "exclude": ["node_modules", "lib"]
  }