		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.2",
		"storybook": "^8.0.10",
		"storybook-css-modules-preset": "^1.1.1",
		"typescript": "^5.4.5",
		"vite": "^5.2.11"
